<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwl.arknight.mapper.SuperiorityMapper">

    <select id="getAllSuperiorityCount" resultType="java.lang.Integer">
        select count(*) from superiority;
    </select>
    <select id="getSuperiority" resultType="com.lwl.arknight.entity.Superiority">
        select * from superiority where number=#{number};
    </select>
</mapper>
